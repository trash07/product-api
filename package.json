{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "",
  "main": "src/server.ts",
  "scripts": {
    "start": "npm run tsc && npm run migration:up:prod && node dist/server.js",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess \"node ./dist/server.js\"",
    "test": "npm run tsc && npm run migration:create:database && npm run migration:up && jasmine-ts 'src/tests/**/*Spec.ts' && npm run migration:drop:database",
    "tsc": "tsc",
    "prettier": "prettier --config .prettierrc.json 'src/**/*.ts' --write",
    "eslint": "eslint 'src/**/*.ts'",
    "migration:up": "ENV=test db-migrate -e test up",
    "migration:up:prod": "ENV=prod db-migrate -e prod up",
    "migration:create:database": "npx . alx-manage-database --create -e test",
    "migration:drop:database": "npx . alx-manage-database --drop -e test"
  },
  "author": "Udacity",
  "license": "ISC",
  "bin": {
    "alx-manage-database": "dist/bin/database.js"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "body-parser": "^1.19.0",
    "dotenv": "^16.0.3",
    "express": "^4.17.1",
    "jsonwebtoken": "^9.0.0",
    "pg": "^8.5.1",
    "typescript": "^4.1.3",
    "yargs": "^17.6.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.9",
    "@types/jasmine": "^3.6.3",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/pg": "^7.14.7",
    "@types/supertest": "^2.0.12",
    "@types/yargs": "^17.0.18",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "eslint": "^8.30.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jasmine": "^3.6.4",
    "jasmine-spec-reporter": "^6.0.0",
    "jasmine-ts": "^0.3.0",
    "prettier": "2.8.1",
    "supertest": "^6.3.3",
    "ts-node": "^10.6.0 ",
    "tsc-watch": "^4.2.9"
  }
}
